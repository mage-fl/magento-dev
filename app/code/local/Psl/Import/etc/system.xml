<?xml version="1.0"?>
<config>
    <sections>
        <psl_import translate="label">
            <label>Import Products</label>
            <tab>catalog</tab>
            <sort_order>1000</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <process translate="label" module="psl_import">
                    <label>Execution Data</label>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>1</expanded>
                    <fields>
                        <fields_mapping translate="label" module="psl_import">
                            <label>Fields mapping</label>
                            <frontend_model>psl_import/system_config_form_field_cataloged</frontend_model>
                            <backend_model>adminhtml/system_config_backend_serialized_array</backend_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </fields_mapping>
                        <non_attributes_values>
                            <label>Extra information</label>
                            <frontend_model>psl_import/system_config_form_field_extra</frontend_model>
                            <backend_model>adminhtml/system_config_backend_serialized_array</backend_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </non_attributes_values>
                        <!--import_values_as translate="label" module="psl_import">
                            <label>Import values as</label>
                            <frontend_type>select</frontend_type>
                            <source_model>psl_import/source_values</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </import_values_as-->
                    </fields>
                </process>
            </groups>
        </psl_import>
    </sections>
</config>